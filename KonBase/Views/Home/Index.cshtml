@{
    ViewData["Title"] = "Sistema para gestão de condomínios";
    Layout = null;
}

<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="author" content="Konbase" />
    <meta name="description" content="Konbase - Sistema para gestão de condomínios" />
    <meta name="keywords" content="Condomínios, Gestão de Condomínios, Síndico, Software para Condomínios, Online">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- SITE TITLE -->
    <title>@ViewData["Title"] - KonBase</title>

    <!-- FAVICON AND TOUCH ICONS  -->
    <link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon">
    <link rel="icon" href="images/favicon.ico" type="image/x-icon">

    <!-- GOOGLE FONTS -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700,800,900" rel="stylesheet">

    <!-- BOOTSTRAP CSS -->
    <link href="~/lib/bootstrap-4.2.1/css/bootstrap.min.css" rel="stylesheet">

    <!-- FONT ICONS -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">
    <!-- TEMPLATE CSS -->
    <link href="~/lib/spinner/css/spinner.css" rel="stylesheet">

    <!-- Style Switcher CSS -->
    <link href="~/css/theme/blue.css" rel="stylesheet" title="blue">

    <!-- RESPONSIVE CSS -->
    <link href="~/css/KonbaseSite.css" rel="stylesheet">



</head>



<body>




    <!-- PRELOADER SPINNER
    ============================================= -->
    <div id="loader-wrapper">
        <div id="loader">
            <div class="cssload-spinner"></div>
        </div>
    </div>

    <!-- PAGE CONTENT
    ============================================= -->
    <div id="page" class="page">




        <!-- HEADER
        ============================================= -->
        <header id="header" class="header">
            <nav class="navbar fixed-top navbar-expand-lg navbar-dark bg-tra hover-menu">
                <div class="container">


                    <!-- LOGO IMAGE -->
                    <!-- For Retina Ready displays take a image with double the amount of pixels that your image will be displayed (e.g 268 x 60 pixels) -->
                    <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand align-items-center justify-content-center logo-white" style="color: #ffffff;">
                        <div class="mx-3"> <i class="fas fa-laugh-wink"></i> <b>Konbase</b></div>
                    </a>

                    <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand align-items-center justify-content-center logo-black" style="color: #000000">
                        <div class=" mx-3"> <i class="fas fa-laugh-wink"></i> <b>Konbase</b></div>
                    </a>

                    <!-- Responsive Menu Button -->
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-bar-icon"><i class="fas fa-bars"></i></span>
                    </button>


                    <!-- Navigation Menu -->
                    <div id="navbarSupportedContent" class="collapse navbar-collapse">
                        <ul class="navbar-nav mr-auto">

                            <li class="nav-item nl-simple"><a class="nav-link" href="#home">Home</a></li>
                            <li class="nav-item nl-simple"><a class="nav-link" href="#services-2">Porque somos diferentes</a></li>
                            <li class="nav-item nl-simple"><a class="nav-link" href="#content-10">Funcionalidades</a></li>
                            <li class="nav-item nl-simple"><a class="nav-link" href="#contacts-1">Contato</a></li>
                            <li class="nav-item nl-simple"><a class="nav-link" href="#pricing-1">Planos</a></li>

                        </ul>
                        <!-- Header Button -->
                        <span class="navbar-text white-color">
                            <a asp-area="" asp-controller="Identity" asp-action="Account/Login" class="btn btn-tra-white black-hover">Entrar</a>
                        </span>

                    </div>	<!-- End Navigation Menu -->


                </div>	  <!-- End container -->
            </nav>	   <!-- End navbar -->
        </header>	<!-- END HEADER -->
        <!-- HERO-2
        ============================================= -->
        <section id="home" class="bg-fixed hero-section division">
            <div class="container">
                <div class="row d-flex align-items-center">


                    <!-- HERO TEXT -->
                    <div class="col-md-6 col-lg-5">
                        <div class="hero-txt white-color">

                            <!-- Title -->
                            <h3 class="h3-xl wow fadeInUp" data-wow-delay="0.5s">
                                Tudo que você precisa para revolucionar sua administração de condomínios
                            </h3>

                            <!-- Text -->
                            <p class="p-md mb-30 wow fadeInUp" data-wow-delay="0.7s">
                                O Sistema <b>Konbase</b> é capaz de alinhar sua rotina administrativa e seus processos gerenciais, trazendo mais mobilidade e praticidade ao seu negócio.
                            </p>

                            <!-- Button -->
                            <a asp-area="" asp-controller="Identity" asp-action="Account/Register" class="btn btn-md btn-theme tra-hover wow fadeInUp" data-wow-delay="0.9s">Experimente por 14 dias gratuitamente.</a>

                        </div>
                    </div>	<!-- END HERO TEXT -->
                    <!-- HERO IMAGE -->
                    <div class="col-md-6 col-lg-6 offset-lg-1">
                        <div class="hero-2-img wow fadeInLeft" data-wow-delay="0.7s">
                            <img class="img-fluid" src="~/images/site/hero-2-img.png" alt="hero-image">
                        </div>
                    </div>


                </div>	  <!-- End row -->
            </div>	   <!-- End container -->
        </section>	<!-- END HERO-2 -->
        <!-- SERVICES-2
        ============================================= -->
        <section id="services-2" class="wide-60 services-section division">
            <div class="container">


                <!-- SECTION TITLE -->
                <div class="row">
                    <div class="col-lg-10 offset-lg-1 section-title">

                        <!-- Title 	-->
                        <h3 class="h3-md">
                            O que faz Konbase ser diferente
                        </h3>

                    </div>
                </div>	 <!-- END SECTION TITLE -->


                <div class="row">


                    <!-- SERVICES BOX #1 -->
                    <div class="col-md-6 col-lg-4 text-center">
                        <div class="sbox-2 box-icon wow fadeInUp" data-wow-delay="0.3s">

                            <!-- Icon -->
                            <img class="img-100" src="~/images/site/icons/e-learning.png" alt="service-icon" />

                            <!-- Title -->
                            <h5 class="h5-sm">
                                Fácil Aprendizado
                            </h5>

                            <!-- Text -->
                            <p class="grey-color">
                                Sistema de fácil aprendizado, e com ajudante de tela.
                            </p>

                        </div>
                    </div>


                    <!-- SERVICES BOX #2 -->
                    <div class="col-md-6 col-lg-4 text-center">
                        <div class="sbox-2 box-icon wow fadeInUp" data-wow-delay="0.5s">

                            <!-- Icon
                            <span class="lightgrey-color flaticon-016-game"></span> -->
                            <!-- Icon -->
                            <img class="img-100" src="~/images/site/icons/responsive.png" alt="service-icon" />

                            <!-- Title -->
                            <h5 class="h5-sm">
                                Layouts Responsivos
                            </h5>

                            <!-- Text -->
                            <p class="grey-color">
                                Se adapta em diferentes tipos de telas.
                            </p>

                        </div>
                    </div>


                    <!-- SERVICES BOX #3 -->
                    <div class="col-md-6 col-lg-4 text-center">
                        <div class="sbox-2 box-icon wow fadeInUp" data-wow-delay="0.7s">

                            <img class="img-100" src="~/images/site/icons/contract.png" alt="service-icon" />

                            <!-- Title -->
                            <h5 class="h5-sm">
                                E-mails ilimitados
                            </h5>

                            <!-- Text -->
                            <p class="grey-color">
                                Não importa o tamanho do seu condomínio ou empresa, envie para os moradores quantos e-mails quiser.
                            </p>

                        </div>
                    </div>


                    <!-- SERVICES BOX #4 -->
                    <div class="col-md-6 col-lg-4 text-center">
                        <div class="sbox-2 box-icon wow fadeInUp" data-wow-delay="0.9s">

                            <img class="img-100" src="~/images/site/icons/settings.png" alt="service-icon" />

                            <!-- Title -->
                            <h5 class="h5-sm">
                                Fácil personalizar
                            </h5>

                            <!-- Text -->
                            <p class="grey-color">
                                Além da sua logo marca, escolha entre as cores, configurações de tela etc.
                            </p>

                        </div>
                    </div>


                    <!-- SERVICES BOX #5 -->
                    <div class="col-md-6 col-lg-4 text-center">
                        <div class="sbox-2 box-icon wow fadeInUp" data-wow-delay="1.1s">

                            <img class="img-100" src="~/images/site/icons/analysis.png" alt="service-icon" />

                            <!-- Title -->
                            <h5 class="h5-sm">Intuitivo</h5>

                            <!-- Text -->
                            <p class="grey-color">
                                Toda tela possui um ajudante pronto para atende-lo e ajuda-lo a lembrar de como fazer algo.
                            </p>

                        </div>
                    </div>


                    <!-- SERVICES BOX #6 -->
                    <div class="col-md-6 col-lg-4 text-center">
                        <div class="sbox-2 box-icon wow fadeInUp" data-wow-delay="1.3s">

                            <img class="img-100" src="~/images/site/icons/support.png" alt="service-icon" />

                            <!-- Title -->
                            <h5 class="h5-sm">
                                Suporte Premium
                            </h5>

                            <!-- Text -->
                            <p class="grey-color">
                                Suporte nota 10 que não te enrola e está sempre disposto a atende-lo.
                            </p>

                        </div>
                    </div>


                </div>	   <!-- End row -->
            </div>	   <!-- End container -->
        </section>	<!-- END SERVICES-2 -->

        <!-- CONTENT-10
        ============================================= -->
        <section id="content-10" class="pb-100 content-section">
            <div class="bg-scroll pt-100 bg-inner blue-abstract division">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12">


                            <!-- CONTENT TEXT -->
                            <div class="content-txt white-color text-center">

                                <!-- Title -->
                                <h3 class="h3-md wow fadeInUp" data-wow-delay="0.3s">
                                    Gerencie seu fluxo de trabalho com o Konbase
                                </h3>

                            </div>	<!-- END CONTENT TEXT -->
                            <!-- CONTENT BOXES -->
                            <div class="content-boxes wow fadeInUp" data-wow-delay="0.7s">
                                <div class="row">


                                    <!-- CONTENT BOX #1 -->
                                    <div class="col-md-6">
                                        <div class="cbox-3 box-icon-sm wow fadeInUp" data-wow-delay="0.9s">

                                            <!-- Icon -->
                                            <img class="img-70" src="~/images/site/icons/design.png" alt="service-icon" />

                                            <!-- Text -->
                                            <div class="cbox-3-txt">

                                                <!-- Title -->
                                                <h5 class="h5-sm">
                                                    Área do Condômino
                                                </h5>

                                                <!-- Text -->
                                                <p>
                                                    Facilite a comunicação com o condômino. Ofereça serviços “self-service”, como a 2º via de boletos, reserva de áreas comuns e prestação de contas online.
                                                </p>

                                            </div>

                                        </div>
                                    </div>	<!-- END CONTENT BOX #1 -->
                                    <!-- CONTENT BOX #2 -->
                                    <div class="col-md-6">
                                        <div class="cbox-3 box-icon-sm wow fadeInUp" data-wow-delay="1.1s">

                                            <!-- Icon -->
                                            <img class="img-70" src="~/images/site/icons/analytics-2.png" alt="service-icon" />

                                            <!-- Text -->
                                            <div class="cbox-3-txt">

                                                <!-- Title -->
                                                <h5 class="h5-sm">
                                                    Dados atualizados em tempo real
                                                </h5>

                                                <!-- Text -->
                                                <p>
                                                    Agende o envio dos boletos. Envie notificações por e-mail ou sms. Automatize processo de cobranças. Faça acordos de forma simples e rápida.
                                                </p>

                                            </div>

                                        </div>
                                    </div>	<!-- END CONTENT BOX #2 -->


                                </div>	<!-- End row -->
                            </div>	<!-- END CONTENT BOXES -->


                        </div>
                    </div>	  <!-- End row -->
                </div>	  <!-- End container -->
            </div>	   <!-- End Inner Background -->
        </section>	<!-- END CONTENT-10 -->


        <!-- PRICING-1
        ============================================= -->
        <section id="pricing-1" class="bg-lightgrey wide-60 pricing-section">
            <div class="container">


                <!-- SECTION TITLE -->
                <div class="row">
                    <div class="col-lg-10 offset-lg-1 section-title">

                        <!-- Title 	-->
                        <h3 class="h3-md">
                            Preços Simples, Uso Instantâneo
                        </h3>

                    </div>
                </div>	 <!-- END SECTION TITLE -->
                <!-- PRICING TABLES -->
                <div class="row pricing-row">


                    <!-- BRONZE PRICING PLAN -->
                    <div class="col-md-3">
                        <div class="pricing-table wow fadeInUp" data-wow-delay="0.3s">

                            <!-- Pricing Plan  -->
                            <div class="pricing-title">
                                <h4 class="h4-xs">Bronze</h4>
                            </div>

                            <!-- Pricing Plan Features  -->
                            <ul class="features">
                                <li>Taxa de Adesão</li>
                                <li>Para 1 Condomínio</li>
                                <li>100MB de espaço</li>
                                <li>8/5 Suporte</li>
                            </ul>

                            <!-- Plan Price  -->
                            <div class="pricing-plan mb-25">
                                <sup>R$</sup>
                                <span class="price">59</span>
                                <sup class="pricing-coins">99</sup>
                                <p class="p-md grey-color">mês</p>
                            </div>

                            <!-- Pricing Table Button  -->
                            <a href="#" class="btn btn-sm btn-tra-black black-hover">Registrar</a>

                        </div>
                    </div>	<!-- END BRONZE PRICING PLAN -->
                    <!-- SILVER PRICING PLAN -->
                    <div class="col-md-3">
                        <div class="pricing-table highlight wow fadeInUp" data-wow-delay="0.5s">

                            <!-- Pricing Plan  -->
                            <div class="pricing-title">
                                <h4 class="h4-xs">Prata</h4>
                            </div>

                            <!-- Pricing Plan Features  -->
                            <ul class="features">
                                <li>Taxa de Adesão</li>
                                <li>Até 5 Condóminios</li>
                                <li>300MB de espaço</li>
                                <li>8/5 Suporte</li>
                            </ul>

                            <!-- Plan Price  -->
                            <div class="pricing-plan mb-25">
                                <sup>R$</sup>
                                <span class="price">129</span>
                                <sup class="pricing-coins">99</sup>
                                <p class="p-md grey-color">monthly</p>
                            </div>

                            <!-- Pricing Table Button  -->
                            <a href="#" class="btn btn-sm btn-theme theme-hover">Registrar</a>

                        </div>
                    </div>	<!-- END SILVER PRICING PLAN -->
                    <!-- GOLD PRICING PLAN -->
                    <div class="col-md-3">
                        <div class="pricing-table wow fadeInUp" data-wow-delay="0.7s">

                            <!-- Pricing Plan  -->
                            <div class="pricing-title">
                                <h4 class="h4-xs">Ouro</h4>
                            </div>

                            <!-- Pricing Plan Features  -->
                            <ul class="features">
                                <li>Taxa de Adesão</li>
                                <li>Até 15 Condóminios</li>
                                <li>700MB de espaço</li>
                                <li>18/7 Suporte Premium</li>
                            </ul>

                            <!-- Plan Price  -->
                            <div class="pricing-plan mb-25">
                                <sup>R$</sup>
                                <span class="price">259</span>
                                <sup class="pricing-coins">99</sup>
                                <p class="p-md grey-color">mês</p>
                            </div>

                            <!-- Pricing Table Button  -->
                            <a href="#" class="btn btn-sm btn-tra-black black-hover">Registrar</a>

                        </div>
                    </div> <!-- END GOLD PRICING PLAN -->
                    <!-- DIAMOND PRICING PLAN -->
                    <div class="col-md-3">
                        <div class="pricing-table wow fadeInUp" data-wow-delay="0.7s">

                            <!-- Pricing Plan  -->
                            <div class="pricing-title">
                                <h4 class="h4-xs">Diamante</h4>
                            </div>

                            <!-- Pricing Plan Features  -->
                            <ul class="features">
                                <li>Sem taxa de adesão</li>
                                <li>Até 25 Condóminios</li>
                                <li>1GB de espaço</li>
                                <li>24/7 Suporte Premium</li>
                            </ul>

                            <!-- Plan Price  -->
                            <div class="pricing-plan mb-25">
                                <sup>R$</sup>
                                <span class="price">419</span>
                                <sup class="pricing-coins">99</sup>
                                <p class="p-md grey-color">mês</p>
                            </div>

                            <!-- Pricing Table Button  -->
                            <a href="#" class="btn btn-sm btn-tra-black black-hover">Registrar</a>

                        </div>
                    </div>	<!-- END DIAMOND PRICING PLAN -->


                </div>	<!-- End row -->
            </div>	   <!-- End container -->
        </section>	<!-- END PRICING-1 -->


        <!-- CALL TO ACTION-4
        ============================================= -->
        <section id="cta-4" class="bg-scroll blue-abstract cta-section division">
            <div class="container white-color">
                <div class="row d-flex align-items-center">


                    <!-- CALL TO ACTION TEXT -->
                    <div class="col-lg-8">
                        <div class="cta-txt">

                            <!-- Title -->
                            <h4 class="h4-lg">Está esperando oque para começar ?</h4>

                            <!-- Text -->
                            <p class="p-md">
                                Registre-se e use por 14 dias gratuitamente.
                            </p>

                        </div>
                    </div>


                    <!-- CALL TO ACTION BUTTON -->
                    <div class="col-lg-3 offset-lg-1">
                        <div class="cta-btn text-right">
                            <a asp-area="" asp-controller="Identity" asp-action="Account/Register" class="btn btn-md btn-theme tra-hover" data-wow-delay="0.9s">Vamos começar</a>
                        </div>
                    </div>


                </div>	 <!-- End row -->
            </div>	   <!-- End container -->
        </section>	<!-- END CALL TO ACTION-4 -->

        <!-- CONTACTS-1
        ============================================= -->
        <section id="contacts-1" class="bg-fixed bg-lightgrey wide-100 contacts-section division">
            <div class="container">


                <!-- SECTION TITLE -->
                <div class="row">
                    <div class="col-lg-10 offset-lg-1 section-title">

                        <!-- Title 	-->
                        <h3 class="h3-md">
                            Não hesite em nos contatar
                        </h3>

                    </div>
                </div>


                <!-- CONTACT FORM -->
                <div class="row">
                    <div class="col-md-10 col-lg-8 offset-md-1 offset-lg-2">
                        <div class="form-holder">
                            <form name="contactform" class="row contact-form">

                                <!-- Contact Form Input -->
                                <div id="input-name" class="col-lg-6">
                                    <input type="text" name="name" class="form-control name" placeholder="Nome">
                                </div>

                                <div id="input-email" class="col-lg-6">
                                    <input type="text" name="email" class="form-control email" placeholder="Email">
                                </div>

                                <div id="input-message" class="col-lg-12 input-message">
                                    <textarea class="form-control message" name="message" rows="6" placeholder="Sua Mensagem ..."></textarea>
                                </div>

                                <!-- Contact Form Button -->
                                <div class="col-lg-12 mt-10 form-btn text-right">
                                    <button type="submit" class="btn btn-md btn-theme theme-hover submit">Enviar Mensagem</button>
                                </div>

                                <!-- Contact Form Message -->
                                <div class="col-lg-12 contact-form-msg">
                                    <span class="loading"></span>
                                </div>

                            </form>
                        </div>
                    </div>
                </div>	   <!-- END CONTACT FORM -->


            </div>	   <!-- End container -->
        </section>	<!-- END CONTACTS-1 -->
        <!-- FOOTER-2
        ============================================= -->
        <footer id="footer-2" class="pt-100 footer division">
            <div class="container">


                <!-- FOOTER CONTENT -->
                <div class="row">


                    <!-- FOOTER INFO -->
                    <div class="col-md-10 col-lg-4">
                        <div class="footer-info mb-40">


                            <!-- Text -->
                            <p>
                                Nos acompanhe nas redes sociais.
                            </p>

                            <!-- Social Icons -->
                            <div class="footer-socials-links">
                                <ul class="foo-socials text-center clearfix">
                                    <li><a href="#" class="ico-facebook"><i class="fab fa-facebook-f"></i></a></li>
                                    <li><a href="#" class="ico-twitter"><i class="fab fa-twitter"></i></a></li>
                                    <li><a href="#" class="ico-linkedin"><i class="fab fa-linkedin-in"></i></a></li>
                                    <li><a href="#" class="ico-tumblr"><i class="fab fa-tumblr"></i></a></li>
                                </ul>
                            </div>

                        </div>
                    </div>


                    <!-- FOOTER PRODUCTS LINKS -->
                    <div class="col-md-3 col-lg-2 offset-lg-1">
                        <div class="footer-links mb-40">

                            <!-- Title -->
                            <h5 class="h5-xs">
                                Produtos
                            </h5>

                            <!-- Footer List -->
                            <ul class="clearfix">
                                <li><p><a href="#">Como funciona?</a></p></li>
                                <li><p><a href="#">Atualizações do produto</a></p></li>
                                <li><p><a href="#">Nossos preços</a></p></li>
                            </ul>

                        </div>
                    </div>


                    <!-- FOOTER COMPANY LINKS -->
                    <div class="col-md-3 col-lg-2">
                        <div class="footer-links mb-40">

                            <!-- Title -->
                            <h5 class="h5-xs">Entrar em contato</h5>

                            <!-- Footer Links -->
                            <ul class="clearfix">
                                <li><p><a href="#">Contate-Nos</a></p></li>
                                <li><p><a href="#">Carreira</a></p></li>
                            </ul>

                        </div>
                    </div>

                    <div class="col-md-3 col-lg-2">
                        <div class="footer-links mb-40">

                            <!-- Title -->
                            <h5 class="h5-xs">Termos de Uso</h5>

                            <!-- Footer Links -->
                            <ul class="clearfix">
                                <li><p><a asp-area="" asp-controller="Home" asp-action="ServiceTerms" >Termos e Condições de Uso</a></p></li>
                            </ul>

                        </div>
                    </div>


                    <!-- FOOTER NEWSLETTER FORM -->
                    <div class="col-md-6 col-lg-3">
                        <div class="footer-form mb-20">

                            <!-- Title -->
                            <h5 class="h5-xs">Siga-nos</h5>

                            <!-- Newsletter Form Input -->
                            <form class="newsletter-form">

                                <div class="input-group">
                                    <input type="email" class="form-control" placeholder="Email">
                                    <span class="input-group-btn">
                                        <button type="submit" class="btn">
                                            <i class="far fa-arrow-alt-circle-right"></i>
                                        </button>
                                    </span>
                                </div>

                                <!-- Newsletter Form Notification -->
                                <label for="s-email" class="form-notification"></label>

                            </form>

                        </div>
                    </div>	<!-- END FOOTER NEWSLETTER FORM -->


                </div>	  <!-- END FOOTER CONTENT -->
                <!-- FOOTER COPYRIGHT -->
                <div class="row bottom-footer">
                    <div class="col-md-12">
                        <div class="footer-copyright text-right">
                            <p>&copy; Designed &#38; Developed By  <span><a href="https://konbase.com.br">Konbase</a></span> 2019 - @DateTime.Today.Year</p>
                        </div>
                    </div>
                </div>


            </div>	   <!-- End container -->
        </footer>	<!-- END FOOTER-2-->




    </div>	<!-- END PAGE CONTENT -->
    <!-- EXTERNAL SCRIPTS
    ============================================= -->
    <script src="~/lib/jquery-3.3.1/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap-4.2.1/js/bootstrap.min.js"></script>
